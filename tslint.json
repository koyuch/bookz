{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": false,
    "interface-name": [true, "never-prefix"],
    "object-literal-sort-keys": false,
    "no-console": {
      "severity": "warning",
      "options": ["log"]
    },
    "arrow-parens": [true, "ban-single-arg-parens"],
    "max-classes-per-file": false
  },
  "rulesDirectory": []
}
